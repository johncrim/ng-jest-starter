{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "cli": {
    "analytics": "7e066299-def0-470c-bb75-1b63c1437357"
  },
  "version": 1,
  "newProjectRoot": "libs",
  "projects": {
    "ui-lib": {
      "projectType": "library",
      "root": "libs/ui-lib",
      "sourceRoot": "libs/ui-lib/src",
      "prefix": "ui",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "libs/ui-lib/tsconfig.lib.json",
            "project": "libs/ui-lib/ng-package.json"
          },
          "configurations": {
            "production": {}
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "libs/ui-lib/test/karma/test.ts",
            "tsConfig": "libs/ui-lib/tsconfig.karma-spec.json",
            "karmaConfig": "libs/ui-lib/karma.conf.js",
            "include": [
              "**/*.karma-spec.ts"
            ]
          }
        }
      }
    },
    "sample-app": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/sample-app",
      "sourceRoot": "apps/sample-app/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/sample-app",
            "index": "apps/sample-app/src/index.html",
            "main": "apps/sample-app/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "apps/sample-app/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "apps/sample-app/src/favicon.ico",
              "apps/sample-app/src/assets"
            ],
            "styles": [
              "apps/sample-app/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "sample-app:build:production"
            },
            "development": {
              "browserTarget": "sample-app:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "sample-app:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "apps/sample-app/tsconfig.karma-spec.json",
            "include": [
              "**/*.karma-spec.ts"
            ],
            "inlineStyleLanguage": "scss",
            "assets": [
              "apps/sample-app/src/favicon.ico",
              "apps/sample-app/src/assets"
            ],
            "styles": [
              "apps/sample-app/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    }
  }
}
